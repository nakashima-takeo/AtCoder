{
    "configurations": [
      {
        "name": "AtCoder: debug",
        "type": "cppdbg",
        "preLaunchTask": "AtCoder: build",
        "request": "launch",
        "program": "${fileDirname}/${fileBasenameNoExtension}.exe",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${fileDirname}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "gdb の再フォーマットを有効にする",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "逆アセンブリ フレーバーを Intel に設定",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
        ]
      },
    ],
    "version": "2.0.0"
}
